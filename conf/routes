# Routes
# ~~~~

GET     /                           controllers.HomeController.index
GET     /create                     controllers.HomeController.none
GET     /admin-ui                   controllers.HomeController.none
GET     /admin-api                  controllers.HomeController.none

GET     /:tenant/admin              controllers.HomeController.none1(tenant)
GET     /:tenant/developers         controllers.HomeController.none1(tenant)

GET     /:tenant/sandbox/:group/*api       controllers.HomeController.none3(tenant, group, api)
GET     /:tenant/:group/*api               controllers.HomeController.none3(tenant, group, api)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
